let n = 20;

if (n % 3 === 0 && n % 5 === 0) {
    console.log("number is divisible by 3 and 5");
} else {
    console.log("number is not divisible 3 and 5 both");
}

console.log();

if (n % 3 === 0 || n % 5 === 0) {
    console.log("number is divisible by 3 or 5.");
} else {
    console.log("number is not divisible 3 and 5.");
}
